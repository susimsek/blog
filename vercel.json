{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/newsletter-dispatch/*.go": {
      "maxDuration": 60
    },
    "api/**/*.go": {
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/newsletter-dispatch",
      "schedule": "0 4 * * *"
    }
  ]
}
