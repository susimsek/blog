{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "description": "Blog application",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "husky",
    "prettier:format": "prettier --write */**/*.{md,json,js,ts,html,tsx,css,scss}",
    "build": "npm run webapp:prod --",
    "webapp:dev": "npm run clean-www && next dev",
    "webapp:prod": "npm run clean-www && npm run webapp:build:prod",
    "webapp:build:prod": "next build",
    "clean-www": "rimraf build/*",
    "sonar": "npx sonar-scanner"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.7.1",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@reduxjs/toolkit": "^2.3.0",
    "bootstrap": "^5.3.3",
    "gray-matter": "^4.0.3",
    "next": "15.0.1",
    "next-i18next": "^15.3.1",
    "next-language-detector": "^1.1.0",
    "react": "18.2.0",
    "react-bootstrap": "^2.10.5",
    "react-dom": "18.2.0",
    "react-i18next": "^15.1.0",
    "react-redux": "^9.1.2",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-bootstrap": "^0.32.37",
    "@types/react-dom": "^18.3.1",
    "@types/react-redux": "^7.1.34",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "i18next-resources-for-ts": "^1.5.0",
    "lint-staged": "^15.2.10",
    "picocolors": "^1.1.1",
    "postcss": "^8",
    "rimraf": "^3.0.2",
    "sass": "1.77.6",
    "sonar-scanner": "^3.1.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=21.6.0"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
